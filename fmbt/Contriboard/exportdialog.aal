tag "export_dialog" {
	guard { return state == "export_dialog" }
	adapter {
	test.wait()
	}

	input "click_done_export" {
	adapter {
	test.clickDoneExport()
	test.wait()
	}
	body {
	state = "board"
	}
	}

	input "click_export" {
	adapter {
	test.clickExport()
	test.wait()
	}
	}

	input "select_export" {
	adapter {
	test.selectExport()
	test.wait()
	}
	}
}